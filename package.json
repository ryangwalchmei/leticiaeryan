{
  "name": "leticiaeryan",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "devonly": "next dev",
    "dev": "npm run services:up && npm run wait-for-postgres && npm run migration:up && next dev",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:down": "docker compose -f infra/compose.yaml down",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "test": "npm run services:up && concurrently -n next,jest --hide next --kill-others -k --success command-jest \"next dev\" \"jest --runInBand --verbose\" ",
    "test:watch": "jest --watchAll --runInBand",
    "migration:create": "node-pg-migrate -m infra/migrations create",
    "migration:up": "node-pg-migrate -m infra/migrations --envPath .env.development up",
    "migration:down": "node-pg-migrate -m infra/migrations --envPath .env.development down",
    "wait-for-postgres": "node infra/scripts/wait-for-postgres.js",
    "lint:eslint:check": "next lint --dir .",
    "prepare": "husky",
    "commit": "cz"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "7.1.0",
    "@mui/material": "7.1.0",
    "@mui/system": "7.1.0",
    "@mui/x-charts": "8.3.1",
    "async-retry": "1.3.3",
    "bootstrap": "5.3.6",
    "classnames": "2.5.1",
    "date-fns": "^4.1.0",
    "dotenv": "16.5.0",
    "dotenv-expand": "12.0.2",
    "fs": "0.0.1-security",
    "next": "15.3.2",
    "node-pg-migrate": "7.9.1",
    "pg": "8.16.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "5.5.0",
    "react-slick": "0.30.3",
    "sass": "1.88.0",
    "sweetalert2": "11.21.0",
    "swr": "2.3.3",
    "xlsx": "0.18.5",
    "yet-another-react-lightbox": "3.23.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "commitizen": "4.3.1",
    "concurrently": "9.1.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.26.0",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-jest": "28.11.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "prettier": "3.5.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

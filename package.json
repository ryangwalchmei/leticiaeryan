{
  "name": "leticiaeryan",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "devonly": "next dev",
    "dev": "npm run services:up && npm run services:wait:database && npm run migrations:up && next dev",
    "test": "npm run services:up && concurrently -n next,jest --hide next --kill-others -k --success command-jest \"next dev\" \"jest --runInBand --verbose\" ",
    "posttest": "npm run services:stop",
    "test:watch": "jest --watchAll --runInBand --verbose",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:down": "docker compose -f infra/compose.yaml down",
    "services:wait:database": "node infra/scripts/wait-for-postgres.js",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "migrations:create": "node-pg-migrate -m infra/migrations create",
    "migrations:up": "node-pg-migrate -m infra/migrations --envPath .env.development up",
    "migrations:down": "node-pg-migrate -m infra/migrations --envPath .env.development down",
    "lint:eslint:check": "next lint --dir .",
    "prepare": "husky",
    "commit": "cz"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@faker-js/faker": "9.8.0",
    "@mui/icons-material": "7.3.6",
    "@mui/material": "7.3.6",
    "@mui/system": "7.3.6",
    "@mui/x-charts": "8.23.0",
    "@tabnews/forms": "^0.4.1",
    "@tabnews/helpers": "^0.4.1",
    "@tabnews/ui": "^0.6.2",
    "async-retry": "1.3.3",
    "bcryptjs": "3.0.3",
    "bootstrap": "5.3.8",
    "classnames": "2.5.1",
    "cookie": "1.1.1",
    "date-fns": "4.1.0",
    "dotenv": "16.5.0",
    "dotenv-expand": "12.0.3",
    "fs": "0.0.1-security",
    "next": "15.2.6",
    "next-connect": "1.0.0",
    "node-pg-migrate": "7.9.1",
    "nodemailer": "7.0.12",
    "pg": "8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "5.5.0",
    "react-slick": "0.30.3",
    "sass": "1.97.1",
    "set-cookie-parser": "2.7.2",
    "simple-react-validator": "^1.6.2",
    "sweetalert2": "11.26.17",
    "swr": "2.3.8",
    "uuid": "11.1.0",
    "xlsx": "0.18.5",
    "yet-another-react-lightbox": "3.28.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "commitizen": "4.3.1",
    "concurrently": "9.2.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.39.2",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "28.11.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "prettier": "3.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

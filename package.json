{
  "name": "leticiaeryan",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "devonly": "next dev",
    "dev": "npm run services:up && npm run wait-for-postgres && npm run migration:up && next dev",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:down": "docker compose -f infra/compose.yaml down",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "test": "npm run services:up && concurrently -n next,jest --hide next --kill-others -k --success command-jest \"next dev\" \"jest --runInBand --verbose\" ",
    "test:watch": "jest --watchAll --runInBand",
    "migration:create": "node-pg-migrate -m infra/migrations create",
    "migration:up": "node-pg-migrate -m infra/migrations --envPath .env.development up",
    "migration:down": "node-pg-migrate -m infra/migrations --envPath .env.development down",
    "wait-for-postgres": "node infra/scripts/wait-for-postgres.js",
    "lint:eslint:check": "next lint --dir .",
    "prepare": "husky",
    "commit": "cz"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "async-retry": "1.3.3",
    "bootstrap": "5.3.3",
    "dotenv": "16.4.7",
    "dotenv-expand": "11.0.6",
    "next": "15.1.0",
    "node-pg-migrate": "6.2.2",
    "pg": "8.13.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "5.5.0",
    "sass": "1.83.4"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "commitizen": "4.3.0",
    "concurrently": "8.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.6.0",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "prettier": "3.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
